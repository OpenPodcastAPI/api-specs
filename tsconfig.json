{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "paths": {
      "@partials/*": [
        "src/content/docs/partials/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  }
}